# ===================================================================
# .env FILE (Place in ROOT directory - same level as package.json)
# Location: /your-project-root/.env
# ===================================================================

# Google Analytics Configuration
REACT_APP_GA_TRACKING_ID=G-YOUR-ACTUAL-TRACKING-ID
REACT_APP_GTM_ID=GTM-PW9PNF87

# Enable/Disable Analytics (set to 'false' to disable in development)
REACT_APP_ENABLE_ANALYTICS=true

# Custom Analytics Endpoint (optional - for your own analytics)
# Set to 'DISABLED' if you don't want custom analytics
REACT_APP_ANALYTICS_ENDPOINT=https://prescreening-tool.onrender.com/analytics

# SEO Configuration
REACT_APP_SITE_URL=https://gigaversity.in
REACT_APP_SITE_NAME=Gigaversity
REACT_APP_DEFAULT_OG_IMAGE=https://gigaversity.in/assets/og-default.jpg

# API Configuration (from your existing api.js)
REACT_APP_API_BASE_URL1=https://airesume.gigaversity.in
REACT_APP_API_BASE_URL2=https://prescreening-tool.onrender.com
REACT_APP_API_BASE_URL3=/pay

# Performance Configuration
REACT_APP_PRELOAD_ROUTES=true
REACT_APP_LAZY_LOAD_IMAGES=true

# Development Configuration
REACT_APP_DEBUG_SEO=false
REACT_APP_SHOW_ANALYTICS_LOGS=false

# Social Media Configuration
REACT_APP_TWITTER_HANDLE=@gigaversity
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id

# Schema.org Configuration
REACT_APP_ORGANIZATION_NAME=Gigaversity
REACT_APP_ORGANIZATION_DESCRIPTION=India's Premier Tech Education Platform
REACT_APP_ORGANIZATION_ADDRESS=Tech Park, Whitefield, Bangalore, Karnataka, India
REACT_APP_ORGANIZATION_PHONE=+91-XXXXXXXXXX